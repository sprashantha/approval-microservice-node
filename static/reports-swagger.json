{
    "swagger": "2.0",
    "info": {
        "version": "v4",
        "title": "Expense reports API.",
        "description": "This set of APIs deals with retrieving, saving, deleting and        submitting expense reports.\n"
    },
    "host": "concur-proxy-env-nb7sjtnmm2.elasticbeanstalk.com",
    "basePath": "/expense/v4",
    "paths": {
        "/links": {
            "get": {
                "tags": [
                    "root"
                ],
                "operationId": "getReportLinks",
                "description": "Gets all the top-level API endpoints for expense reports.\n",
                "responses": {
                    "200": {
                        "description": "An array of API endpoint links. All the API endpoints are listed here.\n",
                        "schema": {
                            "title": "ArrayOfLinks",
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/Link"
                            }
                        }
                    }
                }
            }
        },
        "/users/{userId}/reports": {
            "get": {
                "tags": [
                    "reports"
                ],
                "operationId": "getReportsForUser",
                "description": "Gets reports belonging to a user. These reports may be unsubmitted or submitted and can be filtered by status criteria and time period. The reports are sorted from the most recent to the oldest creation date.\n",
                "parameters": [
                    {
                        "name": "userId",
                        "in": "path",
                        "description": "guid identifying the user",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "status",
                        "in": "query",
                        "description": "Filtering criteria for reports to be retrieved.",
                        "required": false,
                        "type": "string",
                        "enum": [
                            "unsubmitted",
                            "active",
                            "pending",
                            "approved",
                            "processed",
                            "paid",
                            "paymentConfirmed"
                        ]
                    },
                    {
                        "name": "timePeriod",
                        "in": "query",
                        "description": "Time period based on creation date.",
                        "required": false,
                        "type": "string",
                        "enum": [
                            "all",
                            "thisMonth",
                            "lastMonth",
                            "thisQuarter",
                            "lastQuarter",
                            "thisYear",
                            "lastYear"
                        ]
                    },
                    {
                        "name": "size",
                        "in": "query",
                        "description": "Size of array. Has to be at most 500.",
                        "required": false,
                        "type": "number",
                        "format": "double"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "An array of expense reports matching the criteria.\n",
                        "schema": {
                            "title": "ArrayOfReports",
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/Report"
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "reports"
                ],
                "operationId": "createReport",
                "description": "Creates a new report.\n",
                "parameters": [
                    {
                        "name": "report",
                        "in": "body",
                        "description": "Report to add to the system",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/Report"
                        }
                    },
                    {
                        "name": "userId",
                        "in": "path",
                        "description": "guid identifying the user",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Report created.",
                        "schema": {
                            "title": "ReportLink",
                            "description": "A link to the report resource with the URL - https://{host}/expense/v4/users/{userId}/reports/{reportId}\n",
                            "type": "object",
                            "$ref": "#/definitions/Link"
                        }
                    }
                }
            }
        },
        "/users/{userId}/reports/{reportId}": {
            "get": {
                "tags": [
                    "reports"
                ],
                "operationId": "getReportForUser",
                "description": "Gets the details of a report.\n",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "userId",
                        "in": "path",
                        "description": "guid identifying the user",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "reportId",
                        "type": "string",
                        "in": "path",
                        "description": "ID of the report.",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Report details.",
                        "schema": {
                            "title": "Report",
                            "$ref": "#/definitions/Report"
                        }
                    }
                }
            }
        },
        "/users/{userId}/reports/{reportId}/workflow": {
            "get": {
                "tags": [
                    "reports"
                ],
                "operationId": "getReportWorkflow",
                "description": "Gets the submission details of a report including approval status, payment status, approver, etc.\n",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "userId",
                        "in": "path",
                        "description": "guid identifying the user",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "reportId",
                        "type": "string",
                        "in": "path",
                        "description": "ID of the report.",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Workflow status of the report.",
                        "schema": {
                            "title": "Workflow Status",
                            "$ref": "#/definitions/WorkflowStatus"
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "reports"
                ],
                "operationId": "submitReport",
                "description": "Submit the report",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "userId",
                        "in": "path",
                        "description": "guid identifying the user",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "reportId",
                        "type": "string",
                        "in": "path",
                        "description": "ID of the report.",
                        "required": true
                    }
                ],
                "responses": {
                    "202": {
                        "description": "Accepted. Responds with the Workflow Status of the report. The report may be queued for submission. Calling GET on this URL at a later time may return the actual approval status of the report such as Pending Approval.",
                        "schema": {
                            "title": "Workflow Status",
                            "$ref": "#/definitions/WorkflowStatus"
                        }
                    }
                }
            }
        },
        "/users/{userId}/reports/{reportId}/expenses": {
            "get": {
                "tags": [
                    "reports"
                ],
                "operationId": "getExpenses",
                "description": "Gets the expenses associated with a report.\n",
                "parameters": [
                    {
                        "name": "userId",
                        "in": "path",
                        "description": "guid identifying the user",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "reportId",
                        "type": "string",
                        "in": "path",
                        "description": "ID of the report.",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "An array of expenses associated with the report.\n",
                        "schema": {
                            "title": "ArrayOfExpenses",
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/Expense"
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "reports"
                ],
                "operationId": "createExpense",
                "description": "Creates a new expense item in a report.\n",
                "parameters": [
                    {
                        "name": "Expense",
                        "in": "body",
                        "description": "Expense to add to the report",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/Expense"
                        }
                    },
                    {
                        "name": "userId",
                        "in": "path",
                        "description": "guid identifying the user",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "reportId",
                        "in": "path",
                        "description": "ID of the report",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Expense created.",
                        "schema": {
                            "title": "ExpenseLink",
                            "description": "A link to the expense resource with the URL - https://{host}/expense/report/v4/users/{userid}/reports/{reportId}/expenses/{expenseId}\n",
                            "type": "object",
                            "$ref": "#/definitions/Link"
                        }
                    }
                }
            }
        },
        "/users/{userId}/reports/{reportId}/expenses/{expenseId}": {
            "get": {
                "tags": [
                    "reports"
                ],
                "operationId": "getExpense",
                "description": "Gets the expenses associated with a report.\n",
                "parameters": [
                    {
                        "name": "userId",
                        "in": "path",
                        "description": "guid identifying the user",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "reportId",
                        "type": "string",
                        "in": "path",
                        "description": "ID of the report.",
                        "required": true
                    },
                    {
                        "name": "expenseId",
                        "type": "string",
                        "in": "path",
                        "description": "ID of the expense.",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "The Expense associated with the expenseId.\n",
                        "schema": {
                            "title": "Expense",
                            "type": "object",
                            "$ref": "#/definitions/Expense"
                        }
                    }
                }
            }
        },
        "/approvers/{userId}/reports": {
            "get": {
                "tags": [
                    "approvals"
                ],
                "operationId": "getReportsToApprove",
                "description": "Gets reports to approve for an approver. The reports are sorted from the most recent to the oldest  SubmitDate.\n",
                "parameters": [
                    {
                        "name": "userId",
                        "in": "path",
                        "description": "guid identifying the approver",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "size",
                        "in": "query",
                        "description": "Size of array. Has to be at most 500.",
                        "required": false,
                        "type": "number",
                        "format": "double"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "An array of expense reports that are pending approval.\n",
                        "schema": {
                            "title": "ArrayOfReports",
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/Report"
                            }
                        }
                    }
                }
            }
        },
        "/approvers/{userId}/reports/{reportId}": {
            "get": {
                "tags": [
                    "approvals"
                ],
                "operationId": "getReportToApprove",
                "description": "Gets the details of a report.\n",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "userId",
                        "in": "path",
                        "description": "guid identifying the approver",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "reportId",
                        "type": "string",
                        "in": "path",
                        "description": "ID of the report.",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Report details.",
                        "schema": {
                            "title": "Report",
                            "$ref": "#/definitions/Report"
                        }
                    }
                }
            }
        },
        "/approvers/{userId}/reports/{reportId}/workflow": {
            "get": {
                "tags": [
                    "approvals"
                ],
                "operationId": "getApprovalWorkflow",
                "description": "Gets the workflow details of a report for an approver including its approval status, payment status and approver information.\n",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "userId",
                        "in": "path",
                        "description": "guid identifying the approver",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "reportId",
                        "type": "string",
                        "in": "path",
                        "description": "ID of the report.",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Approval status of the report.",
                        "schema": {
                            "title": "Report",
                            "$ref": "#/definitions/WorkflowStatus"
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "approvals"
                ],
                "operationId": "changeWorkflow",
                "description": "Approve or reject the report",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "Approval",
                        "in": "body",
                        "description": "The approval action for the report.",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/ApprovalInfo"
                        }
                    },
                    {
                        "name": "userId",
                        "in": "path",
                        "description": "guid identifying the approver",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "reportId",
                        "type": "string",
                        "in": "path",
                        "description": "ID of the report.",
                        "required": true
                    }
                ],
                "responses": {
                    "202": {
                        "description": "Accepted. Responds with the Workflow Status of the report. The report may be queued for approval or rejection. Calling GET on this URL at a later time may return the actual approval status of the report such as Approved. This also contains the Payment Status of the report and the approver information.",
                        "schema": {
                            "title": "Workflow Status",
                            "$ref": "#/definitions/WorkflowStatus"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "Report": {
            "type": "object",
            "required": [
                "name"
            ],
            "properties": {
                "reportID": {
                    "type": "string",
                    "description": "Unique id of the report."
                },
                "href": {
                    "type": "string",
                    "description": "URL to self-https://{host}/users/{userid}/reports/{reportid}\n"
                },
                "name": {
                    "type": "string",
                    "description": "Name of the report."
                },
                "purpose": {
                    "type": "string",
                    "description": "Business purpose of the report."
                },
                "totalAmount": {
                    "type": "number",
                    "description": "The total amount captured by the expense report in the reimbursement currency represented by the currencyCode field."
                },
                "personalAmount": {
                    "type": "number",
                    "description": "The amount of personal expenses in the reimbursement currency represented by the currencyCode field."
                },
                "claimedAmount": {
                    "type": "number",
                    "description": "The claimed amount is the totalAmount minus the personalAmount."
                },
                "approvedAmount": {
                    "type": "number",
                    "description": "The amount approved for reimbursement by approvers. Approvers can adjust the approval ammount of an expense item (partial approvals)."
                },
                "currencyCode": {
                    "type": "string",
                    "description": "ISO 4217 currency code of the submitter's reimbursement currency."
                },
                "country": {
                    "type": "string",
                    "description": "ISO 3166 country code of the submitter's home country."
                },
                "reportDate": {
                    "type": "string",
                    "description": "User entered date associated with the report in ISO-8601 UTC format. Typically this refers to the date of the earliest expense in the report."
                },
                "createDate": {
                    "type": "string",
                    "description": "Creation date in ISO-8601 UTC format."
                },
                "submitDate": {
                    "type": "string",
                    "description": "The date the report was submitted in ISO-8601 UTC format.\n"
                },
                "paidDate": {
                    "type": "string",
                    "description": "The date the report was paid in ISO-8601 UTC format.\n"
                },
                "lastModifiedDate": {
                    "type": "string",
                    "description": "The last modified date of the report in ISO-8601 UTC format."
                },
                "ledgerName": {
                    "type": "string",
                    "description": "Name of the Ledger used when creating the report. Useful for accounting integration."
                },
                "everSentBack": {
                    "type": "string",
                    "description": "Y if sent back, N if not."
                },
                "hasException": {
                    "type": "string",
                    "description": "Y if the report or any of its line items have a policy exception, N if not."
                },
                "approvalStatus": {
                    "type": "object",
                    "description": "Approval status",
                    "$ref": "#/definitions/ApprovalStatus"
                },
                "paymentStatus": {
                    "type": "object",
                    "description": "Payment status",
                    "$ref": "#/definitions/PaymentStatus"
                },
                "approver": {
                    "type": "object",
                    "description": "Approver for whom the report is pending approval. Only relevant if the approvalStatus.statusName is Pending Approval.",
                    "$ref": "#/definitions/User"
                },
                "owner": {
                    "type": "object",
                    "description": "Owner of the report.",
                    "$ref": "#/definitions/User"
                },
                "workflowAction": {
                    "type": "object",
                    "description": "Hyperlink to allowed workflow actions (submit, approve, reject)",
                    "$ref": "#/definitions/Link"
                },
                "expenses": {
                    "type": "object",
                    "description": "Expense line items in the report.",
                    "$ref": "#/definitions/ExpensesLink"
                },
                "orgUnits": {
                    "type": "array",
                    "description": "Array of org units",
                    "items": {
                        "type": "string",
                        "maxItems": 10
                    }
                },
                "customFields": {
                    "type": "array",
                    "description": "Array of custom fields",
                    "items": {
                        "$ref": "#/definitions/CustomField",
                        "maxItems": 20
                    }
                }
            }
        },
        "WorkflowStatus": {
            "type": "object",
            "required": [
                "approvalStatus"
            ],
            "properties": {
                "submitDate": {
                    "type": "string",
                    "description": "The date the report was submitted in ISO-8601 UTC format.\n"
                },
                "approvalStatus": {
                    "type": "object",
                    "description": "Approval status",
                    "$ref": "#/definitions/ApprovalStatus"
                },
                "paymentStatus": {
                    "type": "object",
                    "description": "Payment status",
                    "$ref": "#/definitions/PaymentStatus"
                },
                "approver": {
                    "type": "object",
                    "description": "Approver for whom the report is pending approval. Only relevant if the approvalStatus.statusName is Pending Approval.",
                    "$ref": "#/definitions/User"
                }
            }
        },
        "ApprovalStatus": {
            "type": "object",
            "required": [
                "statusCode",
                "statusName"
            ],
            "properties": {
                "statusCode": {
                    "type": "string",
                    "enum": [
                        "A_NOTF",
                        "A_RESU",
                        "A_PEND",
                        "A_APPR"
                    ]
                },
                "statusName": {
                    "type": "string",
                    "enum": [
                        "Not filed",
                        "Resubmit",
                        "Pending Approval",
                        "Approved"
                    ]
                }
            }
        },
        "PaymentStatus": {
            "type": "object",
            "required": [
                "statusCode",
                "statusName"
            ],
            "properties": {
                "statusCode": {
                    "type": "string",
                    "enum": [
                        "P_NOTP",
                        "P_PEND",
                        "P_PAID"
                    ]
                },
                "statusName": {
                    "type": "string",
                    "enum": [
                        "Not paid",
                        "Pending payment",
                        "Paid"
                    ]
                }
            }
        },
        "ApprovalInfo": {
            "type": "object",
            "required": [
                "action"
            ],
            "properties": {
                "action": {
                    "type": "string",
                    "description": "Approve or Reject.",
                    "enum": [
                        "APPROVE",
                        "SEND_BACK"
                    ]
                },
                "comment": {
                    "type": "string",
                    "description": "Optional comment by the approver. Required if rejecting the report."
                },
                "approvers": {
                    "type": "array",
                    "description": "Optionally specify an array of approvers to route the report to. This is in the case of dynamic workflow.\n",
                    "items": {
                        "$ref": "#/definitions/User"
                    }
                }
            }
        },
        "User": {
            "type": "object",
            "required": [
                "name",
                "loginid"
            ],
            "properties": {
                "name": {
                    "type": "string",
                    "description": "Name of the user."
                },
                "loginid": {
                    "type": "string",
                    "description": "loginid of the user."
                },
                "href": {
                    "type": "string",
                    "description": "URL to approver of the form https://{host}/userprofile/v4/users/{userid}\n"
                }
            }
        },
        "ExpensesLink": {
            "type": "object",
            "required": [
                "href"
            ],
            "properties": {
                "href": {
                    "type": "string",
                    "description": "Link to expenses-https://{host}/expense/v4/reports/{reportId}/expenses\n"
                }
            }
        },
        "Expense": {
            "type": "object",
            "required": [
                "expenseTypeID",
                "transactionAmount",
                "currencyCode",
                "transactionDate"
            ],
            "properties": {
                "href": {
                    "type": "string",
                    "description": "URL to self-https://{host}/users/{userid}/reports/{reportId}/expenses/{expenseId} \n"
                },
                "expenseTypeID": {
                    "type": "string",
                    "description": "Generated code of the expense type."
                },
                "expenseTypeName": {
                    "type": "string",
                    "description": "Name of the expense type in the supplied locale. Defaults to English."
                },
                "spendCategory": {
                    "type": "string",
                    "description": "Used in analytics this refers to a grouping of expense types."
                },
                "transactionDate": {
                    "type": "string",
                    "description": "Transaction date in ISO-8601 UTC format."
                },
                "lastModifiedDate": {
                    "type": "string",
                    "description": "Last modified date date in ISO-8601 UTC format."
                },
                "transactionAmount": {
                    "type": "number",
                    "description": "The requested amount of the expense in the transaction currency."
                },
                "postedAmount": {
                    "type": "number",
                    "description": "The requested amount in the reimbursement currency."
                },
                "approvedAmount": {
                    "type": "number",
                    "description": "The approved amount of the expense in the reimbursement currency."
                },
                "currencyCode": {
                    "type": "string",
                    "description": "ISO 4217 currency code of the transaction currency."
                },
                "currencyName": {
                    "type": "string",
                    "description": "Name of the transaction currency in the supplied locale."
                },
                "exchangeRate": {
                    "type": "number",
                    "description": "Exchange rate between the transaction corrency and the report's reimbursement currency."
                },
                "paymentTypeCode": {
                    "type": "string",
                    "description": "CASH or CARD."
                },
                "paymentTypeName": {
                    "type": "string",
                    "description": "Name of the payment type in the supplied locale. Defaults to English."
                },
                "vendorDescription": {
                    "type": "string",
                    "description": "The name or description of the Vendor where the expense was incurred."
                },
                "locationName": {
                    "type": "string",
                    "description": "Name of the city where the expense happened."
                },
                "locationSubdivision": {
                    "type": "string",
                    "description": "The state or province where the expense happened."
                },
                "locationCountry": {
                    "type": "string",
                    "description": "The country where the expense happened."
                },
                "receiptImage": {
                    "type": "object",
                    "description": "Hyperlink to the receipt image.",
                    "$ref": "#/definitions/Link"
                },
                "eReceipt": {
                    "type": "object",
                    "description": "Hyperlink to vendor supplied electronic receipt.",
                    "$ref": "#/definitions/Link"
                },
                "expenseForm": {
                    "type": "object",
                    "description": "Hyperlink to the expense form.",
                    "$ref": "#/definitions/Link"
                },
                "isPersonal": {
                    "type": "string",
                    "description": "Y if the expense is personal, else N."
                },
                "hasVAT": {
                    "type": "string",
                    "description": "Y if there are associated VAT transactions,else N."
                },
                "hasComments": {
                    "type": "string",
                    "description": "Y if there are associated comments,else N."
                },
                "hasExceptions": {
                    "type": "string",
                    "description": "Y if there are associated policy Exceptions,else N."
                },
                "hasAttendees": {
                    "type": "string",
                    "description": "Y if there are associated attendees,else N."
                },
                "hasAllocations": {
                    "type": "string",
                    "description": "Y if there are associated allocation transactions, else N."
                },
                "isCreditCardCharge": {
                    "type": "string",
                    "description": "Y if it is a credit card transaction,else N."
                },
                "isPersonalCardCharge": {
                    "type": "string",
                    "description": "Y if it is a personal credit card transaction,else N."
